{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/build-profile.html", "/front.html", "/landingpage.html", "/*.{css,js,png,jpg,gif,svg,ico}"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' https:",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html"
  }
}